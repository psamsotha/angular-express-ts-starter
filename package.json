{
  "name": "angular-express-ts-starter",
  "version": "1.0.0",
  "description": "A start project for Angular, Express, Webpack, and TypeScript",
  "main": "index.js",
  "scripts": {
    "start": "npm run run:client",
    "clean": "rimraf dist",
    "build": "npm run clean && webpack --config ./config/webpack.build.js --progress --profile --bail --display-chunks --hide-modules",
    "build:client": "webpack --config ./config/webpack.client.js --progress --profile --bail --display-chunks --hide-modules",
    "build:server": "webpack --config ./config/webpack.server.js --progress --profile --bail --display-chunks --hide-modules",
    "run:client": "webpack-dev-server --config ./config/webpack.client.js --inline --progress --port 8080",
    "run:server": "node ./dist/server.js",
    "test": "npm run clean && npm run test:server && npm run test:client",
    "test:server": "mocha-webpack --webpack-config ./config/webpack.server.js \"src/backend/**/*.spec.ts\"",
    "test:server:watch": "npm run test:server -- --watch",
    "test:client": "karma start",
    "test:client:watch": "npm run test:client -- --no-single-run --auto-watch"
  },
  "author": "Paul Samsotha <paulsamsotha@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "core-js": "^2.4.1",
    "express": "^4.15.2",
    "helmet": "^3.5.0",
    "inversify": "^3.3.0",
    "inversify-express-utils": "^3.3.0",
    "morgan": "^1.8.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.2.0",
    "winston": "^2.3.1",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.1",
    "@types/chai": "^3.4.35",
    "@types/compression": "0.0.33",
    "@types/express": "^4.0.35",
    "@types/helmet": "0.0.34",
    "@types/jasmine": "^2.5.46",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.10",
    "@types/sinon": "^1.16.36",
    "@types/supertest": "^2.0.0",
    "@types/winston": "^2.3.0",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^6.7.7",
    "awesome-typescript-loader": "^3.1.2",
    "chai": "^3.5.0",
    "concurrently": "^3.4.0",
    "css-loader": "^0.27.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "jasmine-core": "^2.5.2",
    "jasmine-spec-reporter": "^3.2.0",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.2.0",
    "mocha-webpack": "^0.7.0",
    "node-sass": "^4.5.1",
    "null-loader": "^0.1.1",
    "postcss-loader": "^1.3.3",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "sinon": "^2.1.0",
    "style-loader": "^0.16.0",
    "supertest": "^3.0.0",
    "ts-helpers": "^1.1.2",
    "typescript": "^2.2.1",
    "webpack": "^2.3.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.5.4"
  }
}
